{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 13
    },
    "rules": {
        "func-names": 0,
        "global-require": 0,
        "import/no-dynamic-require": 0,
        "no-underscore-dangle": 0
    }
}
